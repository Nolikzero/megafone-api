{
    "name": "meshgroup/megafon-notification-channel",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Meshgroup\\Megafon\\": "src/"
        }
    },
    "authors": [
        {
            "name": "noname"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^6.2",
        "illuminate/queue": "5.1 - 5.8",
        "illuminate/notifications": "5.1 - 5.8",
        "illuminate/support": "5.1 - 5.8"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.2",
        "mockery/mockery": "^1.0"
    },
    "autoload-dev": {
        "psr-4": {
            "Meshgroup\\Megafon\\Test\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Meshgroup\\Megafon\\MegafonServiceProvider"
            ]
        }
    }
}
